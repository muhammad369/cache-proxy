<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" 
             x:Class="CacheProxyMockServer.Views.HistoryItemView">

	<Grid RowDefinitions="28" ColumnDefinitions="50,1*,150,70,auto,auto" Margin="10 5 10 5">
		<TextBlock x:Name="txtMethod" Grid.Column="0" Grid.Row="0" VerticalAlignment="Center" />
		<TextBlock x:Name="txtUrl" Grid.Column="1" Grid.Row="0" VerticalAlignment="Center" />
		<TextBlock x:Name="txtTime" Grid.Column="2" Grid.Row="0" VerticalAlignment="Center" />
		<TextBlock x:Name="txtLiveOrCache" Grid.Column="3" Grid.Row="0" VerticalAlignment="Center" />

		<Button x:Name="btnRule" Grid.Column="4" Grid.Row="0"  Content="Rule" Margin="0 0 5 0"/>
		<Button x:Name="btnDetails" Grid.Column="5" Grid.Row="0"  Content="Details"/>
		<!--<Button Click="ResendBtnClick"  Content="Resend"/>-->
		
	</Grid>
	
</UserControl>

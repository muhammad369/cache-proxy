<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" 
             x:Class="CacheProxyMockServer.Views.PagingView">
	<StackPanel Orientation="Horizontal" Spacing="5" Margin="3">
		<Button Content=" &lt; " x:Name="btnPrevious"></Button>
		<TextBox x:Name="txtPageNumber" Text="1 - 10 / 53"></TextBox>
		<Button Content=" &gt; " x:Name="btnNext"></Button>
	</StackPanel>
</UserControl>

<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="CacheProxyMockServer.Views.HistoryItemView">

	<StackPanel Orientation="Horizontal"  Spacing="2">
		<TextBlock Text="{Binding Method}" VerticalAlignment="Center" Width="50"/>
		<TextBlock Text="{Binding Url}" VerticalAlignment="Center" Width="400"/>
		<TextBlock Text="{Binding Time}" VerticalAlignment="Center" Width="70"/>

		<Button Command="{Binding OpenRuleWindow}"  Content="Rule"/>
		<Button Command="{Binding ShowDetails}"  Content="Details"/>
		<!--<Button Click="ResendBtnClick"  Content="Resend"/>-->
		
	</StackPanel>
	
</UserControl>
